{
  "name": "unmanned-shop-server",
  "version": "1.0.0",
  "description": "unmanned-shop-server",
  "private": true,
  "egg": {
    "typescript": true,
    "require": [
      "egg-ts-helper/register"
    ]
  },
  "scripts": {
    "start": "egg-scripts start --daemon --title=egg-server-egg-starter  --sticky",
    "stop": "egg-scripts stop --title=egg-server-egg-starter",
    "dev": "egg-bin dev  --sticky",
    "debug": "egg-bin debug",
    "test-local": "egg-bin test",
    "test": "npm run lint -- --fix && npm run test-local",
    "cov": "egg-bin cov",
    "tsc": "ets && tsc -p tsconfig.json",
    "ci": "npm run lint && npm run cov && npm run tsc",
    "autod": "autod",
    "int": "npm run tsc && npm run start",
    "lint": "tslint --project . -c tslint.json",
    "clean": "ets clean"
  },
  "dependencies": {
    "@alicloud/sms-sdk": "^1.1.6",
    "ali-oss": "^6.1.1",
    "apollo-server": "^2.4.8",
    "bcrypt": "^5.0.0",
    "dataloader": "^1.4.0",
    "egg": "^2.6.1",
    "egg-cors": "^2.2.0",
    "egg-curl": "^1.0.1",
    "egg-gql": "0.0.7",
    "egg-jwt": "^3.1.6",
    "egg-redis": "^2.4.0",
    "egg-scripts": "^2.6.0",
    "egg-sequelize": "^4.3.1",
    "egg-socket.io": "^4.1.6",
    "egg-wechat-pay": "^2.2.0",
    "md5": "^2.2.1",
    "moment": "^2.24.0",
    "mysql2": "^1.6.5",
    "qr-image": "^3.2.0",
    "sequelize-cursor-pagination": "^1.3.0",
    "tslint-config-prettier": "^1.18.0",
    "urlencode": "^1.1.0",
    "xml2json": "^0.11.2"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.40",
    "@types/node": "^7.0.12",
    "@types/supertest": "^2.0.0",
    "autod": "^3.0.1",
    "autod-egg": "^1.1.0",
    "egg-ci": "^1.8.0",
    "egg-bin": "^4.6.2",
    "egg-mock": "^3.16.0",
    "egg-ts-helper": "^1.11.0",
    "tslib": "^1.9.0",
    "tslint": "^5.0.0",
    "tslint-config-egg": "^1.0.0",
    "typescript": "^3.0.0"
  },
  "engines": {
    "node": ">=8.9.0"
  },
  "ci": {
    "version": "8"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "eslintIgnore": [
    "coverage"
  ],
  "author": "",
  "license": "MIT"
}
